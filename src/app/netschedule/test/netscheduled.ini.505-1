[server]
port=$PORT
max_connections=1000
max_threads=5
init_threads=2

log=true
log_batch_each_job=true
log_notification_thread=false
log_cleaning_thread=true
log_statistics_thread=true
log_execution_watcher_thread=false

network_timeout=180
admin_client_name=netschedule_admin, netschedule_control
node_id=dev_4_14_0


[log]
file=netscheduled.log

[bdb]
path=$DBPATH
transaction_log_path=./tlog
mutex_max=100000
mem_size=15M
private_env=true
max_locks=100000
max_lockers=25000
max_lockobjects=100000
direct_db=false
direct_log=false
max_queues=5

