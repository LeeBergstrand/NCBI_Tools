<?xml version="1.0"?>
    <Blast4-get-search-strategy-reply xmlns="http://www.ncbi.nlm.nih.gov">
      <Blast4-request>
        <body>
          <Blast4-request-body>
            <queue-search>
              <Blast4-queue-search-request>
                <program>blastn</program>
                <service>plain</service>
                <queries>
                  <Blast4-queries>
                    <seq-loc-list>
                      <Seq-loc>
                        <whole>
                          <Seq-id>
                            <gi>555</gi>
                          </Seq-id>
                        </whole>
                      </Seq-loc>
                    </seq-loc-list>
                  </Blast4-queries>
                </queries>
                <subject>
                  <Blast4-subject>
                    <database>ecoli</database>
                  </Blast4-subject>
                </subject>
                <algorithm-options>
                  <Blast4-parameters>
                    <Blast4-parameter>
                      <name>EvalueThreshold</name>
                      <value>
                        <Blast4-value>
                          <cutoff>
                            <Blast4-cutoff>
                              <e-value>10</e-value>
                            </Blast4-cutoff>
                          </cutoff>
                        </Blast4-value>
                      </value>
                    </Blast4-parameter>
                    <Blast4-parameter>
                      <name>HitlistSize</name>
                      <value>
                        <Blast4-value>
                          <integer>100</integer>
                        </Blast4-value>
                      </value>
                    </Blast4-parameter>
                    <Blast4-parameter>
                      <name>UngappedMode</name>
                      <value>
                        <Blast4-value>
                          <!-- <boolean value="false"/> -->
                          <boolean>false</boolean>
                        </Blast4-value>
                      </value>
                    </Blast4-parameter>
                    <Blast4-parameter>
                      <name>GapOpeningCost</name>
                      <value>
                        <Blast4-value>
                          <integer>5</integer>
                        </Blast4-value>
                      </value>
                    </Blast4-parameter>
                    <Blast4-parameter>
                      <name>GapExtensionCost</name>
                      <value>
                        <Blast4-value>
                          <integer>2</integer>
                        </Blast4-value>
                      </value>
                    </Blast4-parameter>
                    <Blast4-parameter>
                      <name>FilterString</name>
                      <value>
                        <Blast4-value>
                          <string>L;</string>
                        </Blast4-value>
                      </value>
                    </Blast4-parameter>
                    <Blast4-parameter>
                      <name>WordSize</name>
                      <value>
                        <Blast4-value>
                          <integer>11</integer>
                        </Blast4-value>
                      </value>
                    </Blast4-parameter>
                    <Blast4-parameter>
                      <name>EffectiveSearchSpace</name>
                      <value>
                        <Blast4-value>
                          <big-integer>0</big-integer>
                        </Blast4-value>
                      </value>
                    </Blast4-parameter>
                    <Blast4-parameter>
                      <name>HspRangeMax</name>
                      <value>
                        <Blast4-value>
                          <integer>100</integer>
                        </Blast4-value>
                      </value>
                    </Blast4-parameter>
                    <Blast4-parameter>
                      <name>Culling</name>
                      <value>
                        <Blast4-value>
                          <!-- <boolean value="false"/> -->
                          <boolean>false</boolean>
                        </Blast4-value>
                      </value>
                    </Blast4-parameter>
                    <Blast4-parameter>
                      <name>WindowSize</name>
                      <value>
                        <Blast4-value>
                          <integer>0</integer>
                        </Blast4-value>
                      </value>
                    </Blast4-parameter>
                    <Blast4-parameter>
                      <name>MBTemplateLength</name>
                      <value>
                        <Blast4-value>
                          <integer>0</integer>
                        </Blast4-value>
                      </value>
                    </Blast4-parameter>
                    <Blast4-parameter>
                      <name>MBTemplateType</name>
                      <value>
                        <Blast4-value>
                          <integer>0</integer>
                        </Blast4-value>
                      </value>
                    </Blast4-parameter>
                    <Blast4-parameter>
                      <name>MismatchPenalty</name>
                      <value>
                        <Blast4-value>
                          <integer>-3</integer>
                        </Blast4-value>
                      </value>
                    </Blast4-parameter>
                    <Blast4-parameter>
                      <name>MatchReward</name>
                      <value>
                        <Blast4-value>
                          <integer>1</integer>
                        </Blast4-value>
                      </value>
                    </Blast4-parameter>
                    <Blast4-parameter>
                      <name>StrandOption</name>
                      <value>
                        <Blast4-value>
                          <strand-type>
                            <Blast4-strand-type value="both-strands"/>
                          </strand-type>
                        </Blast4-value>
                      </value>
                    </Blast4-parameter>
                    <Blast4-parameter>
                      <name>FirstDbSeq</name>
                      <value>
                        <Blast4-value>
                          <integer>0</integer>
                        </Blast4-value>
                      </value>
                    </Blast4-parameter>
                    <Blast4-parameter>
                      <name>FinalDbSeq</name>
                      <value>
                        <Blast4-value>
                          <integer>0</integer>
                        </Blast4-value>
                      </value>
                    </Blast4-parameter>
                  </Blast4-parameters>
                </algorithm-options>
              </Blast4-queue-search-request>
            </queue-search>
          </Blast4-request-body>
        </body>
      </Blast4-request>
    </Blast4-get-search-strategy-reply>
